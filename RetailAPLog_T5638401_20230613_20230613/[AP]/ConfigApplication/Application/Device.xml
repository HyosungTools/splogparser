      <XmlParam Key="HostNoteTypeD" Value="CAD 50" />
      <XmlParam Key="HostNoteTypeE" Value="CAD 100" />
    </XmlParameters>
    <Condition source="DataDictionary" section="System" key="CountryName" value="CAN" operator="EQU" />
  </XmlSection>
  <XmlSection Name="CashDispenser">
    <XmlParameters>
      <XmlParam Key="HostNoteTypeA" Value="MXN 20" />
      <XmlParam Key="HostNoteTypeB" Value="MXN 50" />
      <XmlParam Key="HostNoteTypeC" Value="MXN 100" />
      <XmlParam Key="HostNoteTypeD" Value="MXN 200" />
      <XmlParam Key="HostNoteTypeE" Value="MXN 500" />
      <XmlParam Key="HostNoteTypeF" Value="MXN 1000" />
    </XmlParameters>
    <Condition source="DataDictionary" section="System" key="CountryName" value="MEX" operator="EQU" />
  </XmlSection>
  <XmlSection Name="CashDispenser">
    <XmlParameters>
      <XmlParam Key="HostNoteTypeA" Value="AUD 100" />
      <XmlParam Key="HostNoteTypeB" Value="AUD 50" />
      <XmlParam Key="HostNoteTypeC" Value="AUD 20" />
      <XmlParam Key="HostNoteTypeD" Value="AUD 10" />
