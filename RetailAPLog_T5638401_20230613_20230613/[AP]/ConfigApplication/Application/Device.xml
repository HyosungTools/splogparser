<?xml version="1.0" encoding="utf-8"?>
<XmlConfiguration xmlns="http://www.nh.com/Config">
  <XmlSection Name="DeviceGeneral">
    <XmlParameters>
      <XmlParam Key="ConfiguredDevice" Value="PIN,DOR,IDC,SPR,GUD,SNS,IND,AUX,VDM,CDM" />
      <XmlParam Key="GBRMSupporting" Value="FALSE" />
