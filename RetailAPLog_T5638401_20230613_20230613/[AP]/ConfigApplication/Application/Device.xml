    <Condition conditionsOperator="OR">
      <Condition source="DataDictionary" section="System" key="ModelName" value="8100QTN" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="ModelName" value="8100T" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="ModelName" value="8200QT" operator="EQU" />
