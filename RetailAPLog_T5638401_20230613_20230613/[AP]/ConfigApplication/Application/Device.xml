	  <XmlParam Key="HostNoteTypeC" Value="NZD 10" />
	</XmlParameters>
	<Condition source="DataDictionary" section="System" key="CountryName" value="NZL" operator="EQU" />
  </XmlSection>
  <XmlSection Name="DeviceGeneral">
    <XmlParameters>
      <XmlParam Key="GBRMSupporting" Value="TRUE" />
    </XmlParameters>
    <Condition conditionsOperator="OR">
      <Condition source="DataDictionary" section="System" key="ModelName" value="8100QTN" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="ModelName" value="8100T" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="ModelName" value="8200QT" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="ModelName" value="8200QTN" operator="EQU" />
    </Condition>
  </XmlSection>
  <XmlSection Name="CashDispenser">
    <XmlParameters>
      <XmlParam Key="DeviceType" Value="BRM" Option="CDM|BRM" />
