      <XmlParam Key="DoubleMACMasterKeyName" Value="MACK3" Option="" />
      <XmlParam Key="DoubleMasterTR31KeyName" Value="MASTERk3" Option=""  />
    </XmlParameters>
  </XmlSection>
  <XmlSection Name="PinPad">
    <XmlParameters>
      <XmlParam Key="KeyMode" Value="3DES3MAC" Option="3DES|3DESMAC|3DES3MAC" />
      <XmlParam Key="DoubleMACMasterKeyName" Value="MASTERK3" />
    </XmlParameters>
    <Condition conditionsOperator="OR">
      <Condition source="DataDictionary" section="System" key="CountryName" value="AUS" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="CountryName" value="NZL" operator="EQU" />
    </Condition>
  </XmlSection>
  <XmlSection Name="EMV">
    <XmlParameters>
      <XmlParam Key="TerminalDataFile" Value="TERM_DATA.ini" />
      <XmlParam Key="AIDInfo" Value="" />      
      <XmlParam Key="KernelType" Value="7" Option="1|2|5|6|7" />
      <XmlParam Key="TransactionTypeOfAAC" Value="30" />
      <XmlParam Key="AdditionalARC" Value="3030,3835" />
      <XmlParam Key="IniDirectory" Value="D:\EMVIni" />
      <XmlParam Key="LogDirectory" Value="D:\NHTrace" />
      <XmlParam Key="KernelDebug" Value="False" />
      <XmlParam Key="EnableCommonAID" Value="TRUE" />
    </XmlParameters>
  </XmlSection>
  <XmlSection Name="EMV">
    <XmlParameters>
      <XmlParam Key="EnableCommonAID" Value="FALSE" />
    </XmlParameters>
    <Condition source="DataDictionary" section="System" key="CountryName" value="CAN" operator="EQU" />
  </XmlSection>

  <!-- 9700 -->
  <XmlSection Name="DeviceGeneral">
    <XmlParameters>
      <XmlParam Key="ConfiguredDevice" Value="PIN,DOR,IDC,SPR,GUD,SNS,IND,AUX,VDM,CDM,BNA" />
    </XmlParameters>
    <Condition conditionsOperator="AND">
      <Condition source="DataDictionary" section="System" key="ModelName" value="9700" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="KioskDispenser" value="True" operator="EQU"/>
    </Condition>
  </XmlSection>
  <XmlSection Name="DeviceGeneral">
    <XmlParameters>
      <XmlParam Key="ConfiguredDevice" Value="PIN,DOR,IDC,SPR,GUD,SNS,IND,AUX,VDM,BNA" />
    </XmlParameters>
    <Condition conditionsOperator="AND">
      <Condition source="DataDictionary" section="System" key="ModelName" value="9700" operator="EQU" />
      <Condition source="DataDictionary" section="System" key="KioskDispenser" value="False" operator="EQU"/>
    </Condition>
  </XmlSection>
  <XmlSection Name="CashDispenser">
    <XmlParameters>
      <XmlParam Key="DeviceType" Value="CDM" Option="CDM|BRM" />
    </XmlParameters>
    <Condition source="DataDictionary" section="System" key="ModelName" value="9700" operator="EQU" />
